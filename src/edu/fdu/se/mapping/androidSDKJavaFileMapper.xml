<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.fdu.se.mapping.androidSDKJavaFileMapper">
    <select id="getAndroidSDKJavaFile" parameterType="int" 
        resultType="edu.fdu.se.bean.AndroidSDKJavaFile">
        select * from android_sdk_java_file where id=#{id}
    </select>
</mapper>